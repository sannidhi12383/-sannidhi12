#include <stdio.h>
int main() {
    char s[] = "RACECAR";
    int i, n;
  n = 7;
    for (i = 0; i < n / 2; i++) {
        if (s[i] != s[n - i - 1]) {
            printf("The string is not a palindrome.\n");
            return 0; 
        }
    }

    printf("The string is a palindrome.\n"); 
    return 0;
}
