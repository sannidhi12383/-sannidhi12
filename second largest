#include<stdio.h>
int main()
{
int arr[5],i,n;
printf("Enter the size of an array:");
scanf("%d",&n);
for(i=0;i<n;i++){
scanf("%d",&arr[i]);
}
for(i=0;i<n;i++){
printf("%d\t\t",arr[i]);
}
int max=arr[0];
for(i=0;i<n;i++){
if(arr[i]>max){
max=arr[i];
}
int smax=arr[0];
for(int i=0;i<n;i++){
if(arr[i]!=max && arr[i]>smax){
smax=arr[i];

}
}
printf("%d\n",smax);
}
}
